    <stall>
        <name>stall</name>
        <mode>looped</mode>
        <path>/Aircraft/767-300/Sounds/stall.wav</path>
        <condition>
            <less-than>
                <property>velocities/airspeed-kt</property>
                <value>100</value>
            </less-than>
            <not>
                <property>/gear/gear[0]/wow</property>
                <property>/gear/gear[1]/wow</property>
                <property>/gear/gear[2]/wow</property>
                <property>/gear/gear[0]/position-norm</property>
            </not>
        </condition>
        <volume>
            <max>0.5</max>
        </volume>
        <position>
            <x>-22</x>
            <y>0</y>
            <z>0</z>
        </position>
        <reference-dist>1.0</reference-dist>
        <max-dist>2.0</max-dist>
    </stall>

    <stall>
        <name>stall</name>
        <mode>looped</mode>
        <path>/Aircraft/767-300/Sounds/stall.wav</path>
        <condition>
            <less-than>
                <property>velocities/airspeed-kt</property>
                <value>120</value>
            </less-than>
            <not>
                <property>/gear/gear[0]/wow</property>
                <property>/gear/gear[1]/wow</property>
                <property>/gear/gear[2]/wow</property>
                <property>/surface-positions/flap-pos-norm</property>
            </not>
        </condition>
        <volume>
            <max>0.5</max>
        </volume>
        <position>
            <x>-22</x>
            <y>0</y>
            <z>0</z>
        </position>
        <reference-dist>1.0</reference-dist>
        <max-dist>2.0</max-dist>
    </stall>

    <switch>
        <name>click on</name>
        <mode>once</mode>
        <path>Aircraft/767-300/Sounds/click.wav</path>
        <condition>
            <property>/sim/sound/switch</property>
        </condition>
        <volume>
            <factor>0.45</factor>
        </volume>
        <position>
            <x>-23</x>
            <y>0</y>
            <z>1</z>
        </position>
        <reference-dist>1.0</reference-dist>
        <max-dist>2.0</max-dist>
    </switch>

    <switch>
        <name>click off</name>
        <mode>once</mode>
        <path>Aircraft/767-300/Sounds/click.wav</path>
        <condition>
            <not>
                <property>/sim/sound/switch</property>
            </not>
        </condition>
        <volume>
            <factor>0.45</factor>
        </volume>
        <position>
            <x>-23</x>
            <y>0</y>
            <z>1</z>
        </position>
        <reference-dist>1.0</reference-dist>
        <max-dist>2.0</max-dist>
    </switch>

    <speedbrake>
        <name>speedbrake</name>
        <mode>in-transit</mode>
        <path>Aircraft/767-300/Sounds/whine.wav</path>
        <volume>
            <factor>0.45</factor>
        </volume>
        <position>
            <x>5</x>
            <y>0</y>
            <z>0</z>
        </position>
        <reference-dist>2.0</reference-dist>
        <max-dist>5.0</max-dist>
    </speedbrake>

    <engines>
        <name>left reverse-thrust</name>
        <mode>looped</mode>
        <path>Aircraft/767-300/Sounds/reverser.wav</path>
        <condition>
            <property>engines/engine[0]/running</property>
        </condition>
        <volume>
            <property>surface-positions/reverser-norm[0]</property>
