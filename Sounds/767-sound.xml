<?xml version="1.0"?>
<!-- Custom 767-300 Sound Effects by Liam Gathercole -->

<PropertyList>

<!-- cockpit x=22.5 y=0 z=0.78   -->

<fx>

<!-- Engines -->

    <engine>
        <name>left engine</name>
        <path>Sounds/jet.wav</path>
        <mode>looped</mode>
        <condition>
            <not>
                <property>controls/engines/engine[0]/cutoff</property>
            </not>
        </condition>
        <volume>
            <property>engines/engine[0]/thrust-lbs</property>
            <factor>0.000015</factor>
            <min>0.0</min>
            <max>1</max>
        </volume>
        <volume>
            <property>sim/sound/Evolume</property>
        </volume>
        <position>
            <x>2.75</x>
            <y>9.8</y>
            <z>-3</z>
        </position>
        <reference-dist>200.0</reference-dist>
        <max-dist>5000.0</max-dist>
    </engine>

    <engine>
        <name>left</name>
        <path>Aircraft/767-300/Sounds/whine.wav</path>
        <mode>looped</mode>
        <property>engines/engine[0]/rpm</property>
        <pitch>
            <property>/engines/engine[0]/rpm</property>
            <offset>0</offset>
            <factor>0.01</factor>
            <min>0.0</min>
            <max>1.0</max>
        </pitch>
        <volume>
            <property>engines/engine[0]/rpm</property>
            <factor>0.1</factor>
            <min>0.0</min>
            <max>0.6</max>
        </volume>
        <volume>
            <property>sim/sound/Evolume</property>
        </volume>
        <position>
            <x>-5.7</x>
            <y>9.8</y>
            <z>-3</z>
        </position>
        <reference-dist>10.0</reference-dist>
        <max-dist>2500.0</max-dist>
    </engine>


    <engine>
        <name>leftwhine2</name>
        <path>Aircraft/767-300/Sounds/whine2.wav</path>
        <mode>looped</mode>
        <property>engines/engine[0]/rpm</property>
    <condition>
      <and>
<not>
        <greater-than>
          <property>/position/altitude-ft</property>
          <value>500</value>
        </greater-than>
</not>
      </and>
    </condition>
        <pitch>
            <property>/engines/engine[0]/rpm</property>
            <offset>0</offset>
            <factor>0.01</factor>
            <min>0.0</min>
            <max>1.0</max>
        </pitch>
        <volume>
            <property>engines/engine[0]/rpm</property>
            <factor>0.1</factor>
            <min>0.0</min>
            <max>0.6</max>
        </volume>
        <volume>
            <property>sim/sound/Evolume</property>
        </volume>
        <position>
            <x>-5.7</x>
            <y>9.8</y>
            <z>-3</z>
        </position>
        <reference-dist>10.0</reference-dist>
        <max-dist>300.0</max-dist>
    </engine>

    <engine>
        <name>right engine</name>
