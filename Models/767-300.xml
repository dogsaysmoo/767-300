<!-- Gear Animation -->
<!-- ############## -->

    <!-- Main gear tilting -->
    <animation>
	<type>rotate</type>
	<object-name>Ltrimpiece7</object-name>
	<object-name>Ltrimpiece8</object-name>
        <object-name>LFtrimTire1</object-name>
        <object-name>LFtrimTire2</object-name>
        <object-name>LFtrimWhell1</object-name>
        <object-name>LFtrimWhell2</object-name>
        <object-name>LBtrimTire1</object-name>
        <object-name>LBtrimTire2</object-name>
        <object-name>LBtrimWhell1</object-name>
        <object-name>LBtrimWhell2</object-name>
	<object-name>Rtrimpiece7</object-name>
	<object-name>Rtrimpiece8</object-name>
        <object-name>RFtrimTire1</object-name>
        <object-name>RFtrimTire2</object-name>
        <object-name>RFtrimWhell1</object-name>
        <object-name>RFtrimWhell2</object-name>
        <object-name>RBtrimTire1</object-name>
        <object-name>RBtrimTire2</object-name>
        <object-name>RBtrimWhell1</object-name>
        <object-name>RBtrimWhell2</object-name>
	<!--property>position/gear-agl-m</property-->
	<property>sim/multiplay/generic/float[4]</property>
	<interpolation>
	    <entry>
		<ind>0</ind>
		<dep>0</dep>
	    </entry>
	    <entry>
		<ind>0.02</ind>
		<dep>0</dep>
	    </entry>
	    <entry>
		<ind>0.75</ind>
		<dep>-20</dep>
	    </entry>
	    <entry>
		<ind>43000</ind>
		<dep>-20</dep>
	    </entry>
	</interpolation>
        <center>
            <x-m> 1.035</x-m>
            <y-m> 0.000</y-m>
            <z-m>-5.780</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <!--       Left Gear      -->
    <animation>
        <type>rotate</type>
        <object-name>left_gear</object-name>
        <object-name>left_gear2</object-name>
        <property>gear/gear[1]/position-norm</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>95</dep>
            </entry>
            <entry>
                <ind>1.0</ind>
                <dep>0</dep>
            </entry>
        </interpolation>
        <center>
            <x-m>6.407</x-m>
            <y-m>-4.300</y-m>
            <z-m>-3.450</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>spin</type>
        <object-name>LFtrimTire1</object-name>
        <object-name>LFtrimTire2</object-name>
        <object-name>LFtrimWhell1</object-name>
        <object-name>LFtrimWhell2</object-name>
        <property>gear/gear[1]/rollspeed-ms</property>
        <factor>-16.213</factor>
        <center>
            <x-m> 0.356</x-m>
            <y-m> 0.000</y-m>
            <z-m>-5.780</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>
    <animation>
        <type>spin</type>
        <object-name>LBtrimTire1</object-name>
        <object-name>LBtrimTire2</object-name>
        <object-name>LBtrimWhell1</object-name>
        <object-name>LBtrimWhell2</object-name>
        <property>gear/gear[1]/rollspeed-ms</property>
        <factor>-16.213</factor>
        <center>
            <x-m> 1.864</x-m>
            <y-m> 0.000</y-m>
            <z-m>-5.780</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <!-- Right Gear -->
    <animation>
        <type>rotate</type>
        <object-name>right_gear</object-name>
        <object-name>right_gear2</object-name>
        <property>gear/gear[2]/position-norm</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>95</dep>
            </entry>
            <entry>
                <ind>1.0</ind>
                <dep>0</dep>
            </entry>
        </interpolation>
        <center>
            <x-m>6.407</x-m>
            <y-m>4.300</y-m>
            <z-m>-3.450</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>spin</type>
        <object-name>RFtrimTire1</object-name>
        <object-name>RFtrimTire2</object-name>
        <object-name>RFtrimWhell1</object-name>
        <object-name>RFtrimWhell2</object-name>
        <property>gear/gear[2]/rollspeed-ms</property>
        <factor>-16.213</factor>
        <center>
            <x-m> 0.356</x-m>
            <y-m> 0.000</y-m>
            <z-m>-5.780</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>
    <animation>
        <type>spin</type>
        <object-name>RBtrimTire1</object-name>
        <object-name>RBtrimTire2</object-name>
        <object-name>RBtrimWhell1</object-name>
        <object-name>RBtrimWhell2</object-name>
        <property>gear/gear[1]/rollspeed-ms</property>
        <factor>-16.213</factor>
        <center>
            <x-m> 1.864</x-m>
            <y-m> 0.000</y-m>
            <z-m>-5.780</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <!-- Front Gear -->
    <animation>
        <type>rotate</type>
        <object-name>nose_gear</object-name>
        <property>gear/gear[0]/position-norm</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>120</dep>
            </entry>
            <entry>
                <ind>0.2</ind>
                <dep>115</dep>
            </entry>
            <entry>
                <ind>1.0</ind>
                <dep>0</dep>
            </entry>
        </interpolation>
        <center>
            <x-m>-22.500</x-m>
            <y-m>0.0</y-m>
            <z-m>-3.550</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
	<type>rotate</type>
        <object-name>LTirenoseGear</object-name>
        <object-name>RTirenoseGear</object-name>
        <object-name>LWhellnoseGear</object-name>
        <object-name>RWhellnoseGear</object-name>
        <object-name>FB3barNG</object-name>
        <object-name>FB4barNG</object-name>
        <object-name>FAbarNG</object-name>
	<condition>
	    <property>gear/gear[0]/wow</property>
	</condition>
	<property>gear/gear[0]/steering-norm</property>
	<factor>-65</factor>
        <center>
            <x-m>-22.100</x-m>
            <y-m>0.0</y-m>
            <z-m>-3.550</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
    </animation>
	
    <animation>
        <type>spin</type>
        <object-name>LTirenoseGear</object-name>
        <object-name>RTirenoseGear</object-name>
        <object-name>LWhellnoseGear</object-name>
        <object-name>RWhellnoseGear</object-name>
        <property>gear/gear[0]/rollspeed-ms</property>
        <factor>-22.656</factor>
        <center>
            <x-m>-22.118</x-m>
            <y-m> 0.000</y-m>
            <z-m>-5.614</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <!-- Gear disapears when raised -->

    <animation>
        <type>select</type>
