<?xml version="1.0"?>

<PropertyList>

    <path>767-300.ac</path>
    <offsets>
        <x-m> 0.0</x-m>
        <y-m> 0.0</y-m>
        <z-m> 0.9</z-m>
        <pitch-deg>-1.0</pitch-deg>
    </offsets>

    <model>
	<name>winglets</name>
	<path>Aircraft/767-300/Models/winglets.ac</path>
    </model>
    <animation>
	<type>select</type>
	<object-name>winglets</object-name>
	<condition>
	    <!--property>sim/model/winglets</property-->
	    <equals>
		<property>sim/model/variant</property>
		<value>1</value>
	    </equals>
	</condition>
    </animation>

<!-- Cockpit, with everything included in the flightdeck directed from this file -->

    <model>
	<name>Cockpit</name>
	<path>Aircraft/767-300/Models/Flightdeck/Flightdeck.xml</path>
	<offsets>
            <x-m>-25.64</x-m>
            <y-m>0</y-m>
            <z-m>-2.38</z-m>
	 </offsets>
    </model>
    <animation>
	<type>select</type>
	<object-name>WindowsFrontMetal</object-name>
	<object-name>Glass_front</object-name>
	<condition>
	    <not>
		<property>sim/current-view/internal</property>
	    </not>
	</condition>
    </animation>
    <animation>
	<type>select</type>
	<object-name>Cockpit</object-name>
	<condition>
		<property>sim/current-view/internal</property>
	</condition>
    </animation>

    <!-- Livery System -->
    <nasal>
        <load>
            var livery_update = aircraft.livery_update.new("Aircraft/767-300/Models/Liveries", 10);
        </load>

        <unload>
            livery_update.stop();
        </unload>
    </nasal>

    <!-- Livery Selection Animation -->
    <animation>
        <type>material</type>
        <object-name>fuselage</object-name>
        <object-name>engines</object-name>
        <object-name>reverser</object-name>
        <object-name>tail</object-name>
        <object-name>VRudder</object-name>
        <object-name>GDL</object-name>
        <object-name>GDR</object-name>
        <object-name>cargodoor</object-name>
        <object-name>WingletL</object-name>
        <object-name>WingletR</object-name>
        <property-base>sim/model/livery</property-base>
        <texture-prop>texture</texture-prop>
        <texture>TFL.png</texture>
    </animation>
    <!-- Livery System -->
    
    <!-- Cockpit, with everything included in the flightdeck directed from this file -->
    <!--model>
        <path>Aircraft/767-300/Models/Cockpit/cockpit.xml</path>
        <offsets>
            <x-m>0.0</x-m>
            <y-m>0.0</y-m>
            <z-m>0.0</z-m>
        </offsets>
    </model-->

    <!-- Pushback -->
    <model>
        <name>Pushback</name>
        <path>Models/Airport/Pushback/Goldhofert.xml</path>
        <condition>
            <less-than-equals>
                <property>velocities/airspeed-kt</property>
                <value>80</value>
            </less-than-equals>
        </condition>
        <offsets>
            <x-m>-22.0</x-m>
            <y-m>-0.03</y-m>
            <z-m>-6.05</z-m>
            <pitch-deg>1.0</pitch-deg>
        </offsets>
    </model>

    <!-- Une ombre pour OSG -->
    <model>
        <name>shadowOSG</name>
        <path>Aircraft/767-300/Models/shadow.xml</path>
        <offsets>
            <x-m> 0.0</x-m>
            <y-m> 0.0</y-m>
            <z-m> 0.0</z-m>
            <pitch-deg>1.2</pitch-deg>
        </offsets>
    </model>

    <animation>
        <type>select</type>
        <object-name>shadowOSG</object-name>
        <condition>
            <equals>
                <property>sim[0]/rendering/shadows-ac</property>
                <value>true</value>
            </equals>
        </condition>
    </animation>
    
